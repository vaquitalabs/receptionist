{
  "name": "spacehorn",
  "version": "1.1.0",
  "description": "Node JS framework to build any website or api with total control and without limiting practices",
  "main": "index.js",
  "bin": "./bin/spacehorn.js",
  "scripts": {
    "build": "babel lib -d dist",
    "build:watch": "babel -w lib -d dist",
    "lint": "eslint .",
    "test": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:raw": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaquitalabs/spacehorn.git"
  },
  "keywords": [
    "spacehorn",
    "framework",
    "express",
    "router"
  ],
  "author": "marcosn <marcos@aftertype.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vaquitalabs/spacehorn/issues"
  },
  "homepage": "https://github.com/vaquitalabs/spacehorn#readme",
  "dependencies": {
    "axios": "0.15.3",
    "body-parser": "1.16.1",
    "chalk": "1.1.3",
    "commander": "^2.12.2",
    "cors": "2.8.1",
    "express": "4.14.1",
    "helmet": "3.4.0",
    "morgan": "1.9.0"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "coveralls": "2.13.1",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.2.0",
    "jest": "19.0.2"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/dist/",
      "/lib/messages/",
      "/node_modules/"
    ]
  }
}
